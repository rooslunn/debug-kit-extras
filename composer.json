{
    "name": "rooslunn/debug-kit-extras",
    "description": "Extended DebugKit plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Eramba Ltd",
            "homepage": "https://www.eramba.org/"
        }
    ],
     "support": {
        "source": "https://github.com/cakephp/bake"
    },
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "DebugKitExtras\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DebugKitExtras\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
